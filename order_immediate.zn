function InitTrig_No_Target takes nothing returns nothing
    set gg_trg_No_Target = CreateTrigger(  )
    call TriggerRegisterAnyUnitEventBJ( gg_trg_No_Target, EVENT_PLAYER_UNIT_ISSUED_ORDER )
    call TriggerAddCondition( gg_trg_No_Target, Condition( function Trig_No_Target_Conditions ) )
    call TriggerAddAction( gg_trg_No_Target, function Trig_No_Target_Actions )
endfunction

