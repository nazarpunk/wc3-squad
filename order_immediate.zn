function Trig_No_Target_Actions takes nothing returns nothing
    call DisableTrigger( GetTriggeringTrigger() )

    // stop/hold
    if ( GetIssuedOrderIdBJ() == String2OrderIdBJ("stop") and IsUnitInGroup(GetTriggerUnit(), udg_Commanders) ) then
        call GroupImmediateOrder( udg_Squad[GetUnitUserData(GetOrderedUnit())], "stop" )
    else
        if ( GetIssuedOrderIdBJ() == String2OrderIdBJ("holdposition") and IsUnitInGroup(GetTriggerUnit(), udg_Commanders) ) then
            call GroupImmediateOrder( udg_Squad[GetUnitUserData(GetOrderedUnit())], "holdposition" )
        endif
    endif
    // ABILITES

    // Укрыться за щитом
    if ( GetIssuedOrderIdBJ() == String2OrderIdBJ("defend") and IsUnitInGroup(GetTriggerUnit(), udg_Commanders)) then
        call GroupImmediateOrder( udg_Squad[GetUnitUserData(GetOrderedUnit())], "defend" )
    endif
    if ( GetIssuedOrderIdBJ() == String2OrderIdBJ("undefend") and IsUnitInGroup(GetTriggerUnit(), udg_Commanders) ) then
        call GroupImmediateOrder( udg_Squad[GetUnitUserData(GetOrderedUnit())], "undefend" )
    endif

    // Огненные стрелы
    if ( GetIssuedOrderIdBJ() == String2OrderIdBJ("flamingarrows") and IsUnitInGroup(GetTriggerUnit(), udg_Commanders) ) then
        call GroupImmediateOrder( udg_Squad[GetUnitUserData(GetOrderedUnit())], "flamingarrows" )
    endif
    if ( Trig_No_Target_Func010C() ) then
        call GroupImmediateOrder( udg_Squad[GetUnitUserData(GetOrderedUnit())], "unflamingarrows" )
    endif

    // Исцеление
    if ( GetIssuedOrderIdBJ() == String2OrderIdBJ("healon") ) then
        call GroupImmediateOrder( udg_SqUniNoCommanderr[GetUnitUserData(GetTriggerUnit())], "healon" )
    endif
    if ( GetIssuedOrderIdBJ() == String2OrderIdBJ("healoff") ) then
        call GroupImmediateOrder( udg_SqUniNoCommanderr[GetUnitUserData(GetTriggerUnit())], "healoff" )
    endif

    // Духовное пламя
    if ( Trig_No_Target_Func015C() ) then
        call GroupImmediateOrder( udg_SqUniNoCommanderr[GetUnitUserData(GetTriggerUnit())], "innerfireon" )
    endif
    if ( Trig_No_Target_Func016C() ) then
        call GroupImmediateOrder( udg_SqUniNoCommanderr[GetUnitUserData(GetTriggerUnit())], "innerfireoff" )
    endif

    // Замедление
    if ( Trig_No_Target_Func018C() ) then
        call GroupImmediateOrder( udg_SqUniNoCommanderr[GetUnitUserData(GetTriggerUnit())], "slowon" )
    endif
    if ( Trig_No_Target_Func019C() ) then
        call GroupImmediateOrder( udg_SqUniNoCommanderr[GetUnitUserData(GetTriggerUnit())], "slowoff" )
    endif
    
    // Рассеить чары
    if ( Trig_No_Target_Func021C() ) then
        call GroupImmediateOrder( udg_SqUniNoCommanderr[GetUnitUserData(GetTriggerUnit())], "autodispelon" )
    endif
    if ( Trig_No_Target_Func022C() ) then
        call GroupImmediateOrder( udg_SqUniNoCommanderr[GetUnitUserData(GetTriggerUnit())], "autodispeloff" )
    endif
    
    // Кровожадность
    if ( Trig_No_Target_Func024C() ) then
        call GroupImmediateOrder( udg_Squad[GetUnitUserData(GetOrderedUnit())], "bloodluston" )
    endif
    if ( Trig_No_Target_Func025C() ) then
        call GroupImmediateOrder( udg_Squad[GetUnitUserData(GetOrderedUnit())], "bloodlustoff" )
    endif
    
    // Ледяные стрелы
    if ( Trig_No_Target_Func027C() ) then
        call GroupImmediateOrder( udg_Squad[GetUnitUserData(GetOrderedUnit())], "coldarrows" )
    endif
    if ( Trig_No_Target_Func028C() ) then
        call GroupImmediateOrder( udg_Squad[GetUnitUserData(GetOrderedUnit())], "uncoldarrows" )
    endif
    
    // Черная стрела
    if ( Trig_No_Target_Func030C() ) then
        call GroupImmediateOrder( udg_Squad[GetUnitUserData(GetOrderedUnit())], "blackarrowon" )
    endif
    if ( Trig_No_Target_Func031C() ) then
        call GroupImmediateOrder( udg_Squad[GetUnitUserData(GetOrderedUnit())], "blackarrowoff" )
    endif
    
    // Канибализм
    if ( Trig_No_Target_Func033C() ) then
        call GroupImmediateOrder( udg_Squad[GetUnitUserData(GetOrderedUnit())], "cannibalize" )
    endif
    
    // Оживление мертвых
    if ( Trig_No_Target_Func035C() ) then
        call GroupImmediateOrder( udg_SqUniNoCommanderr[GetUnitUserData(GetTriggerUnit())], "raisedeadon" )
    endif
    if ( Trig_No_Target_Func036C() ) then
        call GroupImmediateOrder( udg_SqUniNoCommanderr[GetUnitUserData(GetTriggerUnit())], "raisedeadoff" )
    endif
    
    // Проклятье
    if ( Trig_No_Target_Func038C() ) then
        call GroupImmediateOrder( udg_SqUniNoCommanderr[GetUnitUserData(GetTriggerUnit())], "curseon" )
    endif
    if ( Trig_No_Target_Func039C() ) then
        call GroupImmediateOrder( udg_SqUniNoCommanderr[GetUnitUserData(GetTriggerUnit())], "curseoff" )
    endif
    
    // Паутина
    if ( Trig_No_Target_Func041C() ) then
        call GroupImmediateOrder( udg_SqUniNoCommanderr[GetUnitUserData(GetTriggerUnit())], "webon" )
    endif
    if ( Trig_No_Target_Func042C() ) then
        call GroupImmediateOrder( udg_SqUniNoCommanderr[GetUnitUserData(GetTriggerUnit())], "weboff" )
    endif
    
    // Закопаться
    if ( Trig_No_Target_Func044C() ) then
        call GroupImmediateOrder( udg_SqUniNoCommanderr[GetUnitUserData(GetTriggerUnit())], "burrow" )
    endif
    if ( Trig_No_Target_Func045C() ) then
        call GroupImmediateOrder( udg_SqUniNoCommanderr[GetUnitUserData(GetTriggerUnit())], "unburrow" )
    endif
    // Сфинкс
    if ( Trig_No_Target_Func047C() ) then
        call GroupImmediateOrder( udg_SqUniNoCommanderr[GetUnitUserData(GetTriggerUnit())], "avengerform" )
    endif
    // Статуя
    if ( Trig_No_Target_Func049C() ) then
        call GroupImmediateOrder( udg_SqUniNoCommanderr[GetUnitUserData(GetTriggerUnit())], "stoneform" )
    endif
    if ( Trig_No_Target_Func050C() ) then
        call GroupImmediateOrder( udg_SqUniNoCommanderr[GetUnitUserData(GetTriggerUnit())], "unstoneform" )
    endif
    call EnableTrigger( GetTriggeringTrigger() )
endfunction

function Trig_No_Target_Conditions takes nothing returns boolean
    return IsUnitInGroup(GetTriggerUnit(), udg_SqUnCOMMANDER[GetUnitUserData(GetTriggerUnit())])
endfunction

